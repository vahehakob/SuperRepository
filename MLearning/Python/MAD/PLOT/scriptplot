set term epslatex standalone color
set out "tmp.tex"


set yrange [0:100000]

set title '  Total Probability Distribution  '

set xlabel ' clicks in percent '
set ylabel ' number of tokens '

unset clip  one

f(x) = 0

binwidth=0.0001
 bin(x,width)=width*floor(x/width)


plot \
'plot_histosort.dat' using (bin(($1),binwidth)):(1.0) smooth freq with boxes t "sample.tsv"


